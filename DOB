import java.util.*;
class DOB{
    public static void main(String[] args) {
         Scanner sc = new Scanner(System.in);

      
        System.out.println("Enter your birth year  : ");
        int a = sc.nextInt();
        System.out.println("Enter the current year : ");
        int b = sc.nextInt();
       
        int c = b-a;
        
        System.out.println("Your age is "+c);
        
         
    }
}
