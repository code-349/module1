import java.util.*;
class greater {
    public static void main(String[] args) {
       Scanner sc = new Scanner(System.in);
        System.out.println("Enter two number ");
        int a = sc.nextInt();
        int b = sc.nextInt();
        if(a>b){
          System.out.println("a is greater number ");  
        }
        else{
            System.out.println("b is greater number "); 
        }
    }
}
